{
  "name": "contentful-gatsby-boilerplate",
  "version": "2.0.0",
  "description": "Set up boilerplate for simple gatsby-contentful website",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && gatsby build",
    "clean": "rimraf .cache public",
    "contentful:initialize": "node contentful/initialize.js",
    "contentful:tear-down": "node contentful/tear-down.js",
    "start": "gatsby develop -H 0.0.0.0",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "Contentful",
    "Gatsby",
    "Static",
    "Netlify"
  ],
  "author": "jroehl <mail@hinterland.software>",
  "license": "MIT",
  "dependencies": {
    "@contentful/rich-text-types": "^14.1.2",
    "dotenv": "^8.2.0",
    "gatsby": "^2.29.3",
    "gatsby-image": "^2.8.0",
    "gatsby-plugin-react-helmet": "^3.7.0",
    "gatsby-plugin-sharp": "^2.11.2",
    "gatsby-plugin-typescript": "^2.9.0",
    "gatsby-source-contentful": "^4.3.1",
    "gatsby-transformer-sharp": "^2.9.0",
    "lodash": "^4.17.20",
    "lodash.camelcase": "^4.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1"
  },
  "browserslist": [
    "defaults"
