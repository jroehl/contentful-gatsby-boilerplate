{
  "name": "contentful-gatsby-boilerplate",
  "version": "1.0.0",
  "description": "Set up boilerplate for simple gatsby-contentful website",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:static",
    "build:static": "npm run clean && gatsby build",
    "build:dynamic": "NODE_ENV='preview' npm run build:static",
    "clean": "rimraf .cache public",
    "contentful:initialize": "node contentful/initialize.js",
    "contentful:tear-down": "node contentful/tear-down.js",
    "develop": "npm run develop:static",
    "develop:static": "gatsby develop -H 0.0.0.0",
    "develop:dynamic": "NODE_ENV='preview' npm run develop:static",
    "start": "npm run develop",
    "start:static": "npm run develop:static",
    "start:dynamic": "NODE_ENV='preview' npm run start:static",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "jroehl <mail@hinterland.software>",
  "license": "MIT",
  "dependencies": {
    "@contentful/rich-text-from-markdown": "^13.4.0",
    "@contentful/rich-text-plain-text-renderer": "^14.0.0",
    "@contentful/rich-text-react-renderer": "^13.4.0",
    "@contentful/rich-text-types": "^14.1.0",
    "@reach/router": "^1.3.3",
    "contentful": "^7.14.3",
    "contentful-import": "^7.7.11",
    "contentful-management": "^5.26.0",
    "dotenv": "^8.2.0",
    "gatsby": "^2.20.35",
    "gatsby-image": "^2.3.5",
    "gatsby-plugin-react-helmet": "^3.2.5",
    "gatsby-plugin-sharp": "^2.5.7",
    "gatsby-source-contentful": "^2.2.10",
    "gatsby-transformer-sharp": "^2.4.7",
    "lodash.camelcase": "^4.3.0",
    "open": "^7.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "rimraf": "^3.0.2",
    "slug": "^2.1.1"
  },
  "browserslist": [
    "defaults"
  ]
}
